{
  "name": "latteart-capture-cl",
  "version": "2.7.1",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack -w --config webpack.dev.js",
    "test": "jest tests/unit",
    "package": "yarn package:win & yarn package:mac",
    "package:win": "pkg -t node14-win-x64 .",
    "package:mac": "pkg -t node14-macos-x64 .",
    "prepackage:win": "yarn build",
    "prepackage:mac": "yarn build",
    "doc": "typedoc --out dist_docs/typedoc src/",
    "lint": "eslint src",
    "fix": "yarn fix:lint && yarn fix:format",
    "fix:lint": "eslint --fix src tests",
    "fix:format": "prettier --write src tests"
  },
  "bin": {
    "latteart-capture-cl": "dist/index.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.12.70",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "cpx": "^1.5.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "4",
    "jest": "^26.6.2",
    "lint-staged": "^10.5.4",
    "pkg": "^5.8.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.4",
    "typedoc": "^0.23.15",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@types/encoding-japanese": "^1.0.16",
    "@types/express": "4.17.13",
    "@types/selenium-webdriver": "^4.0.15",
    "encoding-japanese": "^1.0.30",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "moment": "^2.29.0",
    "selenium-webdriver": "4.4.0",
    "socket.io": "^3.1.2"
  },
  "resolutions": {
    "glob-parent": "^5.1.2",
    "marked": "^4.0.10",
    "braces": "^2.3.1",
    "@types/express-serve-static-core": "4.17.28"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "pkg": {
    "scripts": [
      "node_modules/selenium-webdriver/**/*.js"
    ],
    "assets": [
      "dist/index.js"
    ],
    "outputPath": "dist/latteart/latteart-capture-cl",
    "name": "latteart-capture-cl"
  },
  "lint-staged": {
    "*": [
      "yarn fix",
      "git add"
    ]
  }
}
